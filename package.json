{
  "name": "react-folder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.4"
  },
  "scripts": {
    "client-install": "npm install --prefix react-folder",
    "start": "node server.js",
    "build": "cd react-folder && npm build",
    "client": "npm start --prefix react-folder",
    "eject": "cd react-folder && npm eject",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix react-folder && npm run build --prefix react-folder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
